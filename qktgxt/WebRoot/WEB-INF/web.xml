<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  	<servlet>
		<servlet-name>zhuce</servlet-name>
		<servlet-class>com.qktgxt.action.zhuce_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>zhuce</servlet-name>
		<url-pattern>/zhuce</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>tgxx</servlet-name>
		<servlet-class>com.qktgxt.action.tgxx_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tgxx</servlet-name>
		<url-pattern>/tgxx</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>zjxx</servlet-name>
		<servlet-class>com.qktgxt.action.zjxx_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>zjxx</servlet-name>
		<url-pattern>/zjxx</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>zbxx</servlet-name>
		<servlet-class>com.qktgxt.action.zbxx_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>zbxx</servlet-name>
		<url-pattern>/zbxx</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>zjsp</servlet-name>
		<servlet-class>com.qktgxt.action.zjsp_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>zjsp</servlet-name>
		<url-pattern>/zjsp</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>zbsp</servlet-name>
		<servlet-class>com.qktgxt.action.zbsp_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>zbsp</servlet-name>
		<url-pattern>/zbsp</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.qktgxt.action.login_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
  	<servlet>
		<servlet-name>yhxx</servlet-name>
		<servlet-class>com.qktgxt.action.yhxx_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>yhxx</servlet-name>
		<url-pattern>/yhxx</url-pattern>
	</servlet-mapping>
	
    <filter>
		<filter-name>SetChartEncoding</filter-name>
		<filter-class>com.qktgxt.util.EncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>SetChartEncoding</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>	
</web-app>
